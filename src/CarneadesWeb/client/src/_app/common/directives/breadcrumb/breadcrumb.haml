.container-fluid{:style => "margin-left: -15px; margin-right: -15px;"}
  .row
    .col-lg-12
      %nav.collapse.navbar-collapse{:role => "navigation"}
        %breadcrumb-entries
          %breadcrumb-entry{"ng-repeat" => "s in states", "state" => "s", "bc-open" => "setCommandView($index)", "index" => "$index"}
        %breadcrumb-commands{"ng-repeat" => "s in states", "ng-show" => "$index == getActiveCommandView()"}
          %ul.nav.nav-pills
            %breadcrumb-command{"ng-repeat" => "c in s.commands", "command" => "c", "bc-change" => "changeState(s.name, $index)"}
